{
  "name": "nft-verification",
  "version": "1.0.0",
  "description": "Discord bot for NFT verification and holder role management",
  "main": "dist/bot.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=0 node dist/bot.js",
    "dev": "ts-node -r tsconfig-paths/register src/bot.ts",
    "prisma:generate": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monaliens/NFTVerification.git"
  },
  "keywords": [
    "discord",
    "bot",
    "nft",
    "verification"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Monaliens/NFTVerification/issues"
  },
  "homepage": "https://github.com/Monaliens/NFTVerification#readme",
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "axios": "^1.6.7",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "module-alias": "^2.2.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.11.20",
    "prisma": "^5.10.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.15",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
